.container
  .row
    .col-sm-10.col-sm-offset-1
      =form_tag({action: :run}, multipart: true, class: "dropzone dz-square", id: "dropzone-form") do
        =hidden_field_tag 'brightness'
        .form-group
          =#file_field_tag 'image'
        .form-group
          =#text_field_tag 'x_cordinate'
        .form-group
          =#text_field_tag 'y_cordinate'
        .form-group
          =#text_field_tag 'width'
        .form-group
          =#text_field_tag 'height'
        .form-group
          =#submit_tag "Get Text"
